			<groupId>com.eharmony.mobile.external</groupId>
			<artifactId>sbjson</artifactId>
			<version>[1.0.1, 1.1.0)</version>
			<name>SBJSON</name>
		</dependency>
        <dependency>
			<groupId>com.eharmony.mobile.external</groupId>
			<artifactId>jsonkit</artifactId>
			<version>[1.2.1, 1.3.0)</version>
			<name>JSONKit</name>
		</dependency>
	</dependencies>

    
</project>